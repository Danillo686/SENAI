    <script>
        var numb = Number(prompt("Escolha um n√∫mero "))
        var segura = numb
        alert (`Vamos contar 10x os multiplos de ${numb}`)
        for (i = 1 ; i <= 9; i++){
            segura = numb + segura
            result = segura + numb
            
            alert (`Contando... ${result}`)
            
        }
    </script>

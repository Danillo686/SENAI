        var numb, contador, i = 0

        while (i < 200){
        i++

        numb = Number(prompt(`Números de 0-200  contador:${i}`))
            if (numb >= 100 && numb >= 200){
                contador++
            }else if (numb == 0){
                alert ("Número errado, resetando o código...")
                i = 200
            }
        }
    </script>

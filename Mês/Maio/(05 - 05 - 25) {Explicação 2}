programa {
  funcao inicio() {
    inteiro numb, i, calc
    //variáveis
    i = 0
    
    escreva ("Escolha um número")
    leia (numb)
    
    enquanto (i <= 9){
      i +=1
      calc = numb * i

      escreva ("\n", calc)
      
    }
/* enquanto "i" for menor que 9 então ele vai ganhar mais 1, o numb não muda, e o calc armazena o numb e usa a contagem do "i" de 1 - 10
então calc é = numb * i 
ex : 2 * 1, 2 * 2, 2 * 3, 2 * 4
*/
  }
}

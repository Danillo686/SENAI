# Atividade1 30/04/25, contador de 1 - 100
programa {
  funcao inicio() {
    inteiro contador, i 
    i = 0
    contador = 0

    enquanto (i < 100){
      contador += 1
      i += 1
      escreva ("\n", "Contando, espere...", contador )
    }
    escreva ("\n", "Sua contagem foi feita com Sucesso!!")
  }
}
